    "d3-dsv": "^1.2.0",
    "d3-geo": "^1.12.1",
    "d3-shape": "^1.3.7",
    "d3-time-format": "^2.2.3",
    "dotenv": "^6.0.0",
    "downshift": "^2.0.0",
